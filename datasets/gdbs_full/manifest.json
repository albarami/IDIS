  "rounding_rules": {
    "currency": "USD",
    "arr_revenue": {
      "precision": 1000,
      "mode": "ROUND_HALF_UP",
      "description": "Round to nearest $1,000"
    },
    "percentages": {
      "precision": 2,
      "mode": "ROUND_HALF_UP",
      "description": "2 decimal places"
    },
    "burn_cac": {
      "precision": 100,
      "mode": "ROUND_HALF_UP",
      "description": "Round to nearest $100"
    },
    "runway": {
      "precision": 1,
      "mode": "ROUND_HALF_UP",
      "description": "1 decimal place (months)"
    },
    "tam": {
      "precision": 1000000,
      "mode": "ROUND_HALF_UP",
      "description": "Round to nearest $1M"
    }
  },
  "claim_set": [
    {
      "claim_key": "C1",
